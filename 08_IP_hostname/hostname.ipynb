   "source": [
    "import socket\n",
    "\n",
    "hostname = input(\"Enter the URL: \").strip()\n",
    "\n",
    "try:\n",
    "    infos = socket.getaddrinfo(hostname, None)\n",
    "    for info in infos:\n",
    "        print(info[4][0])\n",
    "except socket.gaierror:\n",
    "    print(\"Invalid URL or host not found\")\n"
   ]
  }
